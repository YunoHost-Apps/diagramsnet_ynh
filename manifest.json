{
    "name": "Diagrams.net",
    "id": "diagramsnet",
    "packaging_format": 1,
    "description": {
        "en": "Online diagram software for making flowcharts, process diagrams, org charts, UML, ER and network diagrams.",
        "fr": "Application en ligne qui permet de faire des schÃ©mas et du dessin vectoriel."
    },
    "version": "14.6.5~ynh1",
    "url": "https://www.diagrams.net/",
    "license": "Apache-2.0",
    "maintainer": {
        "name": "Gofannon",
        "email": "gofannon@riseup.net"
    },
    "requirements": {
        "yunohost": ">= 4.2.0"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "example": "example.com"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/diagram",
                "default": "/diagram"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": false
            }
        ]
    }
}
